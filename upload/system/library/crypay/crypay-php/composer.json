{
  "name": "crypay/crypay-php",
  "description": "CryPay library for PHP",
  "keywords": [
    "crypay",
    "bitcoin",
    "litecoin",
    "altcoin",
    "merchant",
    "gateway",
    "payment"
  ],
  "homepage": "https://crypay.com",
  "license": "MIT",
  "authors": [
    {
      "name": "CryPay and contributors",
      "homepage": "https://github.com/crypay/crypay-php/graphs/contributors"
    },

    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.0",
    "squizlabs/php_codesniffer": "~3.6.2",
    "phpstan/phpstan": "~1.4.10"
  },
  "autoload": {
    "psr-4": {
      "CryPay\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CryPay\\": [
        "tests/",
        "tests/CryPay/"
      ]
    }
  }
}